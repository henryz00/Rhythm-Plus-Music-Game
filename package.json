{
  "name": "rhythm-plus-game",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "author": {
    "name": "henryz00"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.js\" && eslint --fix \"src/**/*.js\"",
    "lint": "eslint --fix \"src/**/*.js\"",
    "start": "NODE_ENV=development webpack-dev-server --mode development --progress",
    "build:dev": "NODE_ENV=development webpack --mode development",
    "build": "NODE_ENV=production webpack --mode production",
    "clean": "rm -rf ./dist",
    "serve": "yarn run build && serve .",
    "deploy": "yarn build && firebase deploy && git push"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "firebase": "^7.14.3",
    "firebaseui": "^4.5.0",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vue-youtube": "^1.4.0",
    "vuex": "^3.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "vue-loader": "^15.9.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "css-loader": "^3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{html,css,json}": [
      "prettier --write"
    ]
  },
  "license": "ISC"
}
